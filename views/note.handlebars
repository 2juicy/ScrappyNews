{{#each articles}}
<div class="center container">
    <h2>{{title}}</h2>
    <p>{{info}}</p>
    <img class="responsive-img" src="{{img}}" alt="No Image Found">
    <p>
    <a href="http://mmorpg.com/{{link}}">
        <button class='btn btn-large waves-effect waves-light grey darken-3 white-text center'>Read More</button>
    </a>
    <button class='btn btn-large waves-effect waves-light grey darken-3 white-text center'>Comment Post</button>
    </p>
</div>
{{/each}}